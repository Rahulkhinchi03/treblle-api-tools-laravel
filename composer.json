{
    "name": "treblle/treblle-api-tools-laravel",
    "description": "A set of useful tools for building APIs in Laravel.",
    "license": "MIT",
    "homepage": "https://treblle.com/",
    "type": "library",
    "keywords": [
        "api",
        "debuging",
        "documentation",
        "laravel",
        "monitoring",
        "treblle"
    ],
    "authors": [
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://www.juststeveking.uk/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Treblle\\Tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Treblle\\Tools\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "juststeveking/http-helpers": "^0.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.2.3",
        "phpstan/phpstan": "^1.10.8"
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "./vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treblle\\Tools\\Providers\\PackageServiceProvider"
            ],
            "aliases": [
                "Headers"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
